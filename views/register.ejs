<h1>Страница регистрации</h1>
<% if (typeof message !== 'undefined') { %>
    <b><%= message %></b>
<% } %>
<form action="/register" method="POST">
    <input name="inputLogin" placeholder="Логин" required>
    <input type="password" name="inputPassword" placeholder="Пароль" required>
    <input name="inputName" placeholder="ФИО" required>
    <input name="inputType" placeholder="Тип аккаунта" required>
    <input name="inputPosition" placeholder="Класс/должность" required>
    <button type="submit">Регистрация</button>
</form>